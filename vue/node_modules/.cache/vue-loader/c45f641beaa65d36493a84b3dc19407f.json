s()\r\n            },\r\n            postSubmit(param){\r\n                SaveMenu('/index.php/admin/Menu/save',param).then((res)=>{\r\n                    if (res.data.code == 1){\r\n                        this.$message.success(res.data.msg, 2);\r\n                    }else{\r\n                        this.$message.error(res.data.msg, 2);\r\n                    }\r\n                }).catch((error)=>{\r\n                    console.log(error);\r\n                })\r\n            }\r\n        }\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}